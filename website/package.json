{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "npm run validate:all && tsx scripts/syncSkillSubpages.ts && tsx scripts/fetchPlausibleStats.ts && tsx scripts/generateSkillZips.ts && tsx scripts/generateSkillFolderData.ts && tsx scripts/generateSkillMetadata.ts",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "generate:zips": "tsx scripts/generateSkillZips.ts",
    "sync:skills": "tsx scripts/syncSkillDocs.ts",
    "sync:subpages": "tsx scripts/syncSkillSubpages.ts",
    "sync:user-skills": "../scripts/sync-skills.sh",
    "validate:liquid": "node scripts/validate-liquid.js docs/**/*.md",
    "validate:brackets": "node scripts/validate-brackets.js docs/**/*.md",
    "validate:props": "node scripts/validate-skill-props.js docs/skills/*.md",
    "validate:badges": "node scripts/validate-badges.js",
    "validate:all": "npm run validate:liquid && npm run validate:brackets && npm run validate:props && npm run validate:badges",
    "sanitize:mdx": "tsx scripts/sanitize-mdx.ts",
    "sanitize:mdx:fix": "tsx scripts/sanitize-mdx.ts --fix",
    "fix:mdx": "tsx scripts/sanitize-mdx.ts --fix",
    "check:mdx": "tsx scripts/sanitize-mdx.ts",
    "install-hooks": "node scripts/install-git-hooks.js",
    "generate:og": "tsx scripts/generateSkillOgImage.ts --all",
    "generate:og:skill": "tsx scripts/generateSkillOgImage.ts --skill",
    "skills:generate": "tsx scripts/generate-skills.ts",
    "skills:validate": "tsx scripts/generate-skills.ts --validate-only",
    "skills:watch": "tsx scripts/generate-skills.ts --watch",
    "submissions:list": "tsx scripts/batch-process-submissions.ts list",
    "submissions:validate": "tsx scripts/batch-process-submissions.ts validate",
    "submissions:create": "tsx scripts/batch-process-submissions.ts create",
    "submissions:process-all": "tsx scripts/batch-process-submissions.ts process-all",
    "registry:search": "tsx scripts/skill-registry.ts search",
    "registry:import": "tsx scripts/skill-registry.ts import",
    "registry:validate": "tsx scripts/skill-registry.ts validate",
    "registry:publish": "tsx scripts/skill-registry.ts publish",
    "registry:list": "tsx scripts/skill-registry.ts list-remote",
    "generate:bundles": "tsx scripts/generate-bundles.ts",
    "bundles:validate": "tsx scripts/generate-bundles.ts --validate-only",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/diff": "^7.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.24",
    "buffer": "^6.0.3",
    "butterchurn": "^2.6.7",
    "butterchurn-presets": "^2.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "diff": "^8.0.2",
    "docusaurus-plugin-plausible": "^0.0.5",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "midi-player-js": "^2.0.16",
    "midi-writer-js": "^3.1.1",
    "postcss": "^8.5.6",
    "prism-react-renderer": "^2.3.0",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "soundfont-player": "^0.12.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^3.2.4",
    "baseline-browser-mapping": "^2.9.14",
    "chokidar": "^5.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.6.2",
    "vitest": "^3.2.4",
    "yaml": "^2.8.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
